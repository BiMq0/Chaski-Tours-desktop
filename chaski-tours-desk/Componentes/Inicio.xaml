<UserControl x:Class="chaski_tours_desk.Componentes.Inicio"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:chaski_tours_desk.Componentes"
             mc:Ignorable="d" 
             d:DesignHeight="720" d:DesignWidth="1000">
    <StackPanel>
        <StackPanel.Background>
            <ImageBrush ImageSource="../Images/conejo.jpg"
Stretch="UniformToFill"/>
        </StackPanel.Background>

        <Button Width="200" Height="100" Content="Login" FontSize="50" Click="Button_Click" ></Button>

        <Button Width="200" Height="100" Content="Desloguear" FontSize="30" Click="Button_Click_1" ></Button>
    </StackPanel>
</UserControl>
