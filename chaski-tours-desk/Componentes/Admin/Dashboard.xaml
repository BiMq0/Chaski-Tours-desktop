<UserControl x:Class="chaski_tours_desk.Componentes.Admin.Dashboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:chaski_tours_desk.Componentes"
             mc:Ignorable="d" 
             d:DesignHeight="720" d:DesignWidth="280">
    <StackPanel Background="#BF0A30">
        <Image Source="/Images/titulo_blanco.png" Margin="10" />
        <ListBox x:Name="listaBtns" HorizontalAlignment="Center" Width="250" Background="Transparent" BorderThickness="0" SelectionChanged="listaBtns_SelectionChanged">
            <ListBoxItem Content="Inicio"/>
            <ListBoxItem Content="Reservas"/>
            <ListBoxItem Content="Clientes"/>
            <ListBoxItem Content="Tours" />
            <ListBoxItem Content="Sitios"/>
            <ListBoxItem Content="Transportes"/>
            <ListBoxItem Content="Alojamientos"/>
            <ListBoxItem Content="Salidas"/>
        </ListBox>
    </StackPanel>
</UserControl>
